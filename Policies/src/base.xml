<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<TrustFrameworkPolicy PolicySchemaVersion="0.3.0.0" TenantId="__TenantName__" PolicyId="B2C_1A_base" PublicPolicyUri="http://__TenantName__/B2C_1A_base" 
  xmlns="http://schemas.microsoft.com/online/cpim/schemas/2013/06" 
